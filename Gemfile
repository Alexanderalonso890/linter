source 'https://rubygems.org'

# Specify your gem's dependencies in github-lod.gemspec
gemspec :name => ""

gem "rdf", :git => "git://github.com/ruby-rdf/rdf.git", :branch => "develop"

group :development, :test do
  gem 'sparql'
  gem 'rake'
end

group :debug do
  gem 'wirble'
  gem 'debugger'
end
